{% extends 'base.html.twig' %}

{% block body %}
    {% if error %}
        <div>{{ error.messageKey|trans(error.messageData, 'security') }}</div>
    {% endif %}

    {{ form_start(form) }}
    {{ form_row(form._username) }}
    {{ form_row(form._password) }}

    <button type="submit" class="btn btn-default">Login</button>
    {{ form_end(form) }}
{% endblock %}